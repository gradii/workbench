{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/devops/src",
  "prefix": "dt",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "preserveSymlinks": true,
        "outputPath": "dist/apps/devops/dev",
        "index": "apps/devops/src/index.html",
        "main": "apps/devops/src/main.ts",
        "polyfills": "apps/devops/src/polyfills.ts",
        "tsConfig": "apps/devops/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "aot": true,
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules"]
        },
        "assets": ["apps/devops/src/favicon.ico", "apps/devops/src/assets"],
        "styles": [
          "global/nebular-dark/styles.scss",
          "apps/devops/src/styles.scss",
          "apps/devops/src/theme.scss",
          "apps/workbench/src/styles/workbench-styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "outputPath": "dist/apps/devops/prod",
          "fileReplacements": [
            {
              "replace": "apps/devops/src/environments/environment.ts",
              "with": "apps/devops/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "40kb"
            }
          ]
        },
        "uat": {
          "outputPath": "dist/apps/devops/uat",
          "fileReplacements": [
            {
              "replace": "apps/devops/src/environments/environment.ts",
              "with": "apps/devops/src/environments/environment.uat.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "namedChunks": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/devops/src/app/env-module/env.module.ts",
              "with": "apps/devops/src/app/env-module/dev.module.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "disableHostCheck": true,
        "browserTarget": "devops:build",
        "proxyConfig": "apps/devops/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "browserTarget": "devops:build:production"
        },
        "development": {
          "browserTarget": "devops:build:development",
          "proxyConfig": "proxy-config/development.proxy-config.js",
          "port": 8200
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "devops:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/devops/jest.config.ts",
        "tsConfig": "apps/devops/tsconfig.spec.json",
        "passWithNoTests": true,
        "setupFile": "apps/devops/src/test-setup.ts"
      },
      "outputs": ["coverage/apps/devops"]
    }
  },
  "tags": []
}
