{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/workbench/src",
  "prefix": "devops-tools",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/workbench",
        "index": "apps/workbench/src/index.html",
        "main": "apps/workbench/src/main.ts",
        "polyfills": "apps/workbench/src/polyfills.ts",
        "tsConfig": "apps/workbench/tsconfig.app.json",
        "aot": true,
        "stylePreprocessorOptions": {
          "includePaths": ["node_modules"]
        },
        "assets": [
          "apps/workbench/src/favicon.ico",
          "apps/workbench/src/assets"
        ],
        "styles": ["apps/workbench/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/workbench/src/environments/environment.ts",
              "with": "apps/workbench/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": []
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "workbench:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "workbench:build:production"
        },
        "development": {
          "browserTarget": "workbench:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "workbench:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/workbench"],
      "options": {
        "jestConfig": "apps/workbench/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
